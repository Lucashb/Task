@model TaskWeb.Dominio.Indicadores

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @*<title>@ViewBag.Title - My ASP.NET Application</title>*@
    <script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.2/css/bootstrap.min.css" />
  
</head>
<body>
    <div class="row">
        <div class="col-lg-3 col-md-6">
            <div class="card card-inverse card-success">
                <div class="card-block bg-success">
                    <h6 class="text-uppercase">TMA</h6>
                    <h1 class="display-1">@Html.DisplayFor(model => model.tmageral)</h1>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="card card-inverse card-danger">
                <div class="card-block bg-danger">
                    <h6 class="text-uppercase">TMT</h6>
                    <h1 class="display-1">87</h1>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="card card-inverse card-info">
                <div class="card-block bg-info">
                    <h6 class="text-uppercase">Confirmidade Dúvida</h6>
                    <h1 class="display-1">125</h1>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="card card-inverse card-warning">
                <div class="card-block bg-warning">
                    <h6 class="text-uppercase">Confirmidade Geral</h6>
                    <h1 class="display-1">36</h1>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
